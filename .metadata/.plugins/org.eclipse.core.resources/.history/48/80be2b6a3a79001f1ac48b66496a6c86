  <script type="text/javascript">
  function showHutGallery(hut) {
	  var otherHut = (hut == 1) ? 2 : 1;
	  $('hut' + hut).addClass('active'); 
	  $('hut' + otherHut).removeClass('active');
	  $('thumbnails_panel' + otherHut).setStyle('display', 'none');
	  $('slideshow' + otherHut).setStyle('display', 'none');
	  $('thumbnails_panel' + hut).setStyle('display', 'block');
	  $('slideshow' + hut).setStyle('display', 'block');
	  if (hut == 2 && hut2ImageGallery == null) {
		  initHut2Gallery();
	  }	  
  }

  function initHut2Gallery() {
	  hut2ImageGallery = new ImageGallery({
		slides: $$('#slideshow2 img'),
		thumbnails: $$('#thumbnails_wrapper2 img'),
		thumbnailsViewport: 'thumbnails2',
		thumbnailsContainer: 'thumbnails_wrapper2',
		nextThumbnailLink: 'left_arrow2',
		prevThumbnailLink: 'right_arrow2',
		nextThumbnailLinkClass: 'left_arrow',
		prevThumbnailLinkClass: 'right_arrow'});
  }
  </script>
